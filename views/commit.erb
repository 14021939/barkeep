<script src="/js/common.js"></script>
<script src="/js/commit.js"></script>
<div id="commit">
  <% if commit %>
    <div class="comments">
      <% commit.commit_comments.each do |comment| %>
        <%= erb :_comment, :locals => { :comment => comment } %>
      <% end %>
    </div>
    <br/>
  <% end %>
  <% tagged_diff.each do |file| %>
    <%= erb :_diff, :locals => { :commit => commit, :file => file } %>
    <br/>
  <% end %>
</div>
